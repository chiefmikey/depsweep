{
  "name": "stale-deps",
  "version": "1.0.0",
  "description": "CLI tool that identifies and removes unused npm dependencies",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "major": "npm version major; cd dist && npm version major",
    "minor": "npm version minor; cd dist && npm version minor",
    "ncu": "ncu -u; cd dist && ncu -u;",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "async-retry": "^1.3.3",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "find-up": "^7.0.0",
    "globby": "^14.0.2",
    "isbinaryfile": "^5.0.4",
    "micromatch": "^4.0.8",
    "ora": "^8.1.1",
    "readline": "^1.3.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/babel__traverse": "^7.20.6",
    "@types/cli-progress": "^3.11.6",
    "@types/jest": "^29.5.14",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "mikey-pro": "^7.5.3",
    "p-limit": "^6.1.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "stale-deps": "dist/index.js"
  },
  "homepage": "https://github.com/chiefmikey/stale-deps",
  "repository": {
    "type": "git",
    "url": "https://github.com/chiefmikey/stale-deps.git"
  },
  "bugs": {
    "url": "https://github.com/chiefmikey/stale-deps/issues"
  },
  "license": "MIT",
  "keywords": [
    "npm",
    "cli",
    "dependencies",
    "unused",
    "stale",
    "package.json",
    "node"
  ],
  "author": "Mikl Wolfe <wolfe@mikl.io> (https://github.com/chiefmikey)",
  "prettier": "@mikey-pro/prettier-config",
  "eslintConfig": {
    "extends": "@mikey-pro/eslint-config"
  },
  "stylelint": {
    "extends": "@mikey-pro/stylelint-config"
  },
  "engineStrict": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
